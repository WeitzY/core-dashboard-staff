{
  "name": "core-hotels-chat",
  "description": "Core logic and Supabase functions for hotel platform",
  "main": "lib/index.js",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "build": "rimraf lib && tsc",
    "build:watch": "tsc -w",
    "dev": "cross-env NODE_ENV=development ts-node src/scripts/local.ts",
    "watch:dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/scripts/local.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "check": "npm run lint && npm run prettier:check && npm test",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:deploy": "supabase functions deploy",
    "supabase:serve": "supabase functions serve"
  },
  "dependencies": {
    "@langchain/community": "^0.3.47",
    "@langchain/core": "^0.3.61",
    "@langchain/langgraph": "^0.3.5",
    "@langchain/openai": "^0.5.16",
    "@supabase/functions-js": "^2.1.5",
    "@supabase/supabase-js": "^2.49.8",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "gpt-tokenizer": "^2.1.2",
    "openai": "^4.98.0",
    "undici": "^5.28.4",
    "uuid": "^9.0.1",
    "xstate": "^5.19.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "supabase": "^2.30.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  },
  "private": true
}
